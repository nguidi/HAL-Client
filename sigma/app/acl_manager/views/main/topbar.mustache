<div class="navbar navbar-inverse navbar-fixed-top">
	<div class="navbar-inner">
		<div class="container">
			{{#brand}}
			<a href="#" class="brand">
				{{brand}}
			</a>
			{{/brand}}
			<div class="nav-collapse collapse">
				{{#menu}}
					<ul class="nav">
					{{#options}}
						{{#if submenus}}
						<li class="dropdown-submenu">
							<a href="#" {{#self_link}} {{data 'link'}} {{/self_link}}>
								{{title}}
								<span class="caret"></span>
							</a>
							<ul class="dropdown-menu pull-right">
								{{#submenus}}
								<li>
									<a class="browseable " href="#" {{data 'link'}}>
										{{title}}
									</a>
								</li>
								{{/submenus}}
							</ul>
						</li>
						{{else}}
						<li>
							<a href="#" {{#link}} {{data 'link'}} {{/link}} class="browseable">
								{{title}}
							</a>
						</li>
						{{/if}}
					{{/options}}
					</ul>
				{{/menu}}
			</div>
			{{#user}}
			<a class="user dropdown-submenu">
				{{username}}
				<i class="icon-user icon-muted icon-large"></i>
				{{#if submenu}}
				<span class="caret"></span>
				<ul class="dropdown-menu pull-right">
					{{#config}}
					<li>
						<a class="browseable" {{data 'link'}}>
							{{title}}
						</a>
					</li>
					{{/config}}
					{{#signout}}
					<li>
						<a class="browseable signout" {{data 'link'}}>
							{{title}}
						</a>
					</li>
					{{/signout}}
				</ul>
				{{/if}}
			</a>
			{{/user}}
		</div>
	</div>
</div>